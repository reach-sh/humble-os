{
  "name": "humbleswap-server",
  "version": "3.0.2-beta.1",
  "description": "Data-caching layer for HumbleSwap DEx blockchain data",
  "main": "index.js",
  "scripts": {
    "start": "dotenv -e .env.development -- ts-node-dev -r tsconfig-paths/register --transpile-only src/server.ts",
    "start:mainnet": "dotenv -e .env.production -- ts-node-dev -r tsconfig-paths/register --transpile-only src/server.ts",
    "start:staging": "dotenv -e .env.staging -- ts-node-dev -r tsconfig-paths/register --transpile-only src/server.ts",
    "dev:prisma-migrate": "dotenv -e .env.development -- prisma migrate dev",
    "clean": "rm -rf ./lib ./dist",
    "reflect": "NODE_ENV='production' ts-node-dev -r tsconfig-paths/register --transpile-only ./src/graphql",
    "build:pre": "npm run clean && npm run reflect",
    "build": "npm run build:pre && NODE_ENV='production' tsc",
    "build:run": "npm run build && npm run mainnet",
    "mainnet": "NODE_ENV='production' pm2 start ./lib/server.js --cron-restart=\"4 */2 * * *\"",
    "staging": "NODE_ENV='staging' pm2 start ./lib/server.js --cron-restart=\"4 */2 * * *\"",
    "testnet": "NODE_ENV='development' pm2 start ./lib/server.js --cron-restart=\"4 */2 * * *\"",
    "process-nuke": "npm i && npm run build && pm2 stop all && pm2 delete all && pm2 flush",
    "prisma-migrate": "dotenv -e .env.production prisma migrate deploy",
    "push-db": "dotenv -e .env.development prisma db push",
    "restart-pm2": "pm2 flush && pm2 restart all --update-env --cron-restart=\"4 */2 * * *\"",
    "update:git": "git fetch && git pull && npx prisma generate",
    "update:nuke-mainnet": "npm run update:git && npm run process-nuke && npm run mainnet",
    "update:nuke-staging": "npm run update:git && npm run process-nuke && npm run staging",
    "update:nuke-testnet": "npm run update:git && npm run process-nuke && npm run testnet",
    "update:rebuild": "npm run update:git && npm run build && npm run",
    "update:restart": "npm run update:rebuild && npm run restart-pm2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JACK-COM/prisma-express.git"
  },
  "author": "JACK-COM",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JACK-COM/prisma-express/issues"
  },
  "homepage": "https://github.com/JACK-COM/prisma-express#readme",
  "dependencies": {
    "@graphql-tools/schema": "^8.3.13",
    "@prisma/client": "^4.6.0",
    "@reach-sh/humble-sdk": "^3.0.0-beta.18",
    "@reach-sh/stdlib": "0.1.13-rc.0",
    "@types/axios": "^0.14.0",
    "apollo-server": "^3.8.2",
    "axios": "^0.27.2",
    "bcryptjs": "^2.4.3",
    "crypto-js": "^4.1.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-rate-limit": "^6.4.0",
    "graphql": "^16.5.0",
    "graphql-helix": "^1.12.0",
    "graphql-import-node": "^0.0.5",
    "luxon": "^3.2.1",
    "morgan": "^1.10.0",
    "nexus": "^1.3.0",
    "pm2": "^5.2.2",
    "tsconfig-paths": "^4.1.0",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/luxon": "^2.3.2",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.21",
    "nodemon": "^2.0.15",
    "prisma": "^4.6.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.6.2",
    "url-parse": "^1.5.10"
  }
}
