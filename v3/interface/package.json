  "scripts": {
    "start": "GENERATE_SOURCEMAP=false react-app-rewired start",
    "testing": "node cypress/scripts/runTests.js recording",
    "postinstall": "patch-package",
    "build": "cross-env NODE_OPTIONS=--max-old-space-size=6144 GENERATE_SOURCEMAP=false react-app-rewired build",
    "test": "jest --watchAll",
    "test:ci": "jest --silent --ci",
    "cy:run": "cypress run --browser=chrome",
    "test:e2e": "start-server-and-test 'react-app-rewired start' 3000 'cy:run'",
    "eject": "react-scripts eject",
    "extract": "lingui extract",
    "compile": "lingui compile",
    "add-locale": "lingui add-locale",
    "lingui:all": "rm -rf src/locales/ && lingui extract && lingui compile",
    "lint": "eslint src --fix --no-error-on-unmatched-pattern",
    "git-add:all": "git add .",
    "git-add:translations": "git add src/locales/",
    "reach:compile-index": "./reach compile ./src/reach/src/index.rsh",
    "reach:compile-n2nn": "./reach compile ./src/reach/src/n2nn.rsh",
    "reach:compile-announcer": "./reach compile ./src/reach/src/announcer.rsh",
    "reach:compile": "npm run reach:compile-index && npm run reach:compile-n2nn && npm run reach:compile-announcer && node ./src/reach/scripts/renameMjsBuilds.js",
    "pre": "echo \"pre-commit actions are running\"",
    "caa": "node createAlgoAccount.js",
    "auto": "node cypress/scripts/runTests.js",
    "devnet": "./reach down && REACH_CONNECTOR_MODE=ALGO ./reach devnet --await-background"
  },
  "pre-commit": [
    "pre",
    "lint"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "preset": "ts-jest",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/ts-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/cypress"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  },
  "devDependencies": {
    "@lingui/cli": "^3.14.0",
    "@testing-library/cypress": "^9.0.0",
    "@types/await-timeout": "^0.3.1",
    "@types/gtag.js": "^0.0.10",
